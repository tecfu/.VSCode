[
  {
    "key": "ctrl+q",
    "command": "workbench.action.quit"
  },
  {
    "key": "ctrl+shift+down",
    "command": "-workbench.action.terminal.scrollDown",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive"
  },
  {
    "key": "ctrl+shift+up",
    "command": "-workbench.action.terminal.scrollUp",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive"
  },
  {
    "key": "ctrl+shift+j",
    "command": "runCommands",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive",
    "args": {
      "commands": [
        "workbench.action.terminal.scrollDown",
        "workbench.action.terminal.scrollDown"
      ]
    }
  },
  {
    "key": "ctrl+shift+k",
    "command": "runCommands",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive",
    "args": {
      "commands": [
        "workbench.action.terminal.scrollUp",
        "workbench.action.terminal.scrollUp"
      ]
    }
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.terminal.scrollDownPage",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive",
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.terminal.scrollUpPage",
    "when": "terminalFocusInAny && terminalHasBeenCreated && !terminalAltBufferActive || terminalFocusInAny && terminalProcessSupported && !terminalAltBufferActive",
  },
  // {
  //   "key": "a",
  //   "command": "vscodeFileExplorerMenu.addChildNode",
  //   "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  // }
  // {
  //   "key": "d",
  //   "command": "vscodeFileExplorerMenu.deleteCurrentNode",
  //   "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  // },
  // {
  //   "key": "y y",
  //   "command": "vscodeFileExplorerMenu.copyPathToClipboard",
  //   "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  // },
  {
    "key": "enter",
    "command": "-nerdtree.openFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
  },
  {
    "key": "o",
    "command": "runCommands",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus",
    "args": {
      "commands": [
        "explorer.openAndPassFocus",
        "workbench.view.explorer"
      ]
    }
  },
  {
    "key": "ctrl+j",
    "command": "runCommands",
    "when": "listFocus && !inputFocus",
    "args": {
      "commands": [
        "list.focusDown",
        "list.focusDown",
        "list.focusDown",
        "list.focusDown",
        "list.focusDown"
      ]
    }
  },
  {
    "key": "ctrl+k",
    "command": "runCommands",
    "when": "listFocus && !inputFocus",
    "args": {
      "commands": [
        "list.focusUp",
        "list.focusUp",
        "list.focusUp",
        "list.focusUp",
        "list.focusUp"
      ]
    }
  },
  {
    "key": "shift+tab",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+k",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "tab",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+j",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "enter",
    "command": "acceptSelectedSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "ctrl+space",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+w w",
    //"command": "workbench.action.navigateEditorGroups",
    "command": "workbench.view.explorer",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.terminal.clear",
    "when": "terminal.active"
  },
  {
    "key": "shift+w",
    "when": "editorTextFocus == false && !inputFocus",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.files.newUntitledFile"
  },
  // {
  //   "key": "shift+k",
  //   "command": "workbench.action.nextEditorInGroup",
  //   "when": "!(terminalFocus || panelFocus)"
  // },
  // {
  //   "key": "shift+j",
  //   "command": "workbench.action.previousEditorInGroup",
  //   "when": "!(terminalFocus || panelFocus)"
  // },
  {
    "key": "shift+tab shift+k",
    "command": "workbench.action.nextPanelView",
    "when": "panelFocus || terminalFocus || !editorFocus"
  },
  {
    "key": "shift+tab shift+j",
    "command": "workbench.action.previousPanelView",
    "when": "panelFocus || terminalFocus || !editorFocus"
  },
  {
    "key": "ctrl+;",
    "command": "workbench.action.debug.stepInto"
  },
  {
    "key": "ctrl+'",
    "command": "workbench.action.debug.stepOver"
  },
  {
    "key": "ctrl+\\",
    "command": "workbench.action.debug.continue"
  },
  {
    "key": "ctrl+right",
    "command": "workbench.action.debug.stop"
  },
  {
    "key": "cmd+'",
    "command": "workbench.action.debug.stepOver"
  },
  {
    "key": "ctrl+shift+b",
    "command": "editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
    "key": "ctrl+shift+space",
    "command": "-editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
    "key": "ctrl+shift+space",
    "command": "editor.debug.action.toggleBreakpoint"
  },
  {
    "key": "ctrl+shift+j",
    "command": "bookmarks.jumpToNext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+l",
    "command": "-bookmarks.jumpToNext",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+k",
    "command": "bookmarks.jumpToPrevious",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+j",
    "command": "-bookmarks.jumpToPrevious",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+9",
    "command": "workbench.panel.chat.view.copilot.toggleVisibility"
  },
]